#include<stdio.h>
#include<string.h>
main()
{
	char ch[100];
	printf("enter any string:");
	gets(ch);
	int l=strlen(ch),t,c=0,i,j;
	t=l/2;
	j=l-1;
    for(i=0;i<t;i++)
    {
     if(ch[i]==ch[j])
     {
     	c++;
	 }
	 j--;
	}
	if(c==t)
{
	printf("POLINDROME");
	}
else
	{
	printf("NOT PLOINDROME");
	 	}
}

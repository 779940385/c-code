#include<stdio.h>
main()
{
	int row,i,j,s;
	printf("enter how many rows=");
	scanf("%d",&row);
	int a[row][3];
	printf("enter elements into array\n");
	for(i=0;i<row;i++)
	{
	   for(j=0;j<3;j++)
	   {
	   	scanf("%d",&a[i][j]);
	   }
    }
	for(i=0;i<row;i++)
	{
		s=0;
		for(j=0;j<3;j++)
		{
			s=s+a[i][j];
		}
printf("%d row sum=%d\n",i+1,s);
	}
}
